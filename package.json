{
  "name": "vue-knober",
  "title": "VueKnober",
  "version": "1.0.2",
  "description": "A vue knob component",
  "main": "dist/vue-knober.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --config ./build/webpack.config.dev.js",
    "dist:dev": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.dist.dev.js",
    "dist:prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.dist.prod.js",
    "dist": "npm run dist:dev && npm run dist:prod"
  },
  "keywords": [
    "vue-knober",
    "vue",
    "vue.js",
    "knob",
    "vue-component",
    "knober",
    "ui",
    "knob-plate",
    "button",
    "circle",
    "slider",
    "lamp",
    "canvas"
  ],
  "author": "Shaisn <shiasn.ho@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/shiasn/vue-knober"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shiasn/vue-knober/issues"
  },
  "peerDependencies": {
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "babel-loader": "8.0.4",
    "babel-preset-env": "1.7.0",
    "compression-webpack-plugin": "2.0.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
